@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>四川成都明华精品装修公司-预约登记！</title>
    <script src="~/Content/Components/jQuery/jquery-1.12.4.min.js"></script>
    <script src="~/Content/Components/Flexslider/jquery.flexslider.min.js"></script>
    <link href="~/Content/Components/Flexslider/flexslider.css" rel="stylesheet" />
    <link href="~/Content/Css/Areas/MsDec/dec.core.css" rel="stylesheet" />
</head>
<body>
    <!--顶部信息 begin-->
    <div class="dec-panel dec-top">
        <div class="dec-main">
            <div class="dec-top-left">明华装修</div>
            <div class="dec-top-right">
                <a href="javascript:;">咨询热线：13551369057</a>
                <span></span>
                <a href="javascript:;" class="dec-color">我要装修</a>
                <span></span>
                <a href="javascript:;">注册</a>
                <a href="javascript:;">登录</a>
            </div>
        </div>
    </div>
    <!--顶部信息 end-->
    <!--导航信息 begin-->
    <div class="dec-panel dec-nav">
        <div class="dec-main">
            @Html.ActionLink("首页", "Index", new { area = "MsDec", controller = "Home" })
            @Html.ActionLink("效果图", "Index", new { area = "MsDec", controller = "EffPic" })
            @Html.ActionLink("装修攻略", "Index", new { area = "MsDec", controller = "Strategy" })
            @Html.ActionLink("装修报价", "Index", new { area = "MsDec", controller = "Offer" })
            @Html.ActionLink("预约登记", "Index", new { area = "MsDec", controller = "Sub" }, new { @class = "active" })
            @Html.ActionLink("关于我们", "Index", new { area = "MsDec", controller = "About" })
            @Html.ActionLink("联系我们", "Index", new { area = "MsDec", controller = "Contact" })

            <a href="javascript:;">手机装修</a>
        </div>
    </div>
    <!--导航信息 end-->
    <!--子导航信息 begin-->
    <div class="dec-panel dec-subnav">
        <div class="dec-main">
            @Html.ActionLink("首页", "Index", new { area = "MsDec", controller = "Home" })>
            @Html.ActionLink("预约登记", "Index", new { area = "MsDec", controller = "Sub" })
        </div>
    </div>
    <!--子导航信息 end-->
    <!--预约登记 begin-->
    <div class="dec-panel dec-sub">
        <div class="dec-main">
            <table class="frm-tb">
                <tbody>
                    <tr>
                        <td class="title">姓名：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">性别：</td>
                        <td class="content">
                            <label><input type="radio" name="sex" checked="checked" />男</label>
                            <label><input type="radio" name="sex" />女</label>
                        </td>
                    </tr>
                    <tr>
                        <td class="title">姓名：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">电话：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">QQ：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">邮箱：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">楼盘：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">面积：</td>
                        <td class="content"><input type="text" class="txt-main" /></td>
                    </tr>
                    <tr>
                        <td class="title">装修要求：</td>
                        <td class="content"><textarea rows="4" class="txta-main"></textarea></td>
                    </tr>
                    <tr>
                        <td class="sub-lbtn" colspan="2">
                            <a href="javascript:;">提交</a>
                            <a href="javascript:;">重填</a>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <!--预约登记 end-->
    <!--底部 begin-->
    <div class="dec-panel dec-bottom">
        <div class="dec-main">
            <div class="bottom-nav">
                @Html.ActionLink("关于我们", "Index", new { area = "MsDec", controller = "About" })|
                @Html.ActionLink("联系我们", "Index", new { area = "MsDec", controller = "Contact" })|
                <a href="javascript:;">友情链接</a>|
                <a href="javascript:;">帮助中心</a>|
                <a href="javascript:;">意见反馈</a>|
                <a href="javascript:;">法律声明</a>
            </div>
            <div>
                技术支持：Quber，邮箱：qubernet@163.com
            </div>
            <div>
                © 2016 qubernet.com 保留所有权利
            </div>
        </div>
    </div>
    <!--底部 end-->
</body>
</html>
