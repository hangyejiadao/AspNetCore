<script type="text/javascript">

    var paramsData = {
        "LoadApi": '@ViewBag.LoadApi',
        "TargetId": ''
    }

    $(function () {

    });

    function ModuleQuery(data) {
        //alert("查询数据" + JSON.stringify(data));
        var appElement = document.querySelector('[ng-controller=pageCtrl]');
        var $scope = angular.element(appElement).scope();        
        $scope.paramsData.LoadApi = 1;
        $scope.paramsData.TargetId = data.TargetId;
        $scope.items1 = null;
        $scope.items2 = null;
        $scope.items3 = null;
        $scope.$apply();
        $scope.queryData();      
    }


</script>


<script type="text/javascript"> 

    //初始化Angular
    var pageApp = angular.module('pageApp', []);
    pageApp.controller('pageCtrl', function ($scope, $http, $timeout) {
        initNgData($scope, $http);
        initNgEvent($scope, $http, $timeout);
        initNgQuery($scope, $http, $timeout);
    });

    //初始化ng数据
    function initNgData($scope, $http) {

        $scope.paramsData = paramsData;
        $scope.paramsData.TargetId = parent.selData.TargetId;

        var idxData = parent.idxData;
        if (idxData.idx_mod1 == 5 && idxData.idx_mod2 == 2) {
            $scope.paramsData.LoadApi = 1;
        } else {
            $scope.paramsData.LoadApi = 0;
        }

        $scope.model_one = [];
        $scope.model_two =[];
        $scope.model_three = [];
        $scope.model_two.TempletText = "";
 
        $scope.items1 = null;
        $scope.items2 = null;
        $scope.items3 = null;

    }

    //初始化ng事件
    function initNgEvent($scope, $http, $timeout) {
        
        //加载
        $scope.queryData = function () {
            $scope.queryData1();
            $scope.queryData2();
            $scope.queryData3();     
        }

        //加载1
        $scope.queryData1 = function () {
       
            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: true, url: '/MsSys/BhzSys/GetBhzSmsOneData', params: $scope.paramsData }, function (data) {

                //console.log(JSON.stringify(data));

                $scope.model_one = data;

            });
        };

        //加载2
        $scope.queryData2 = function () {

            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: false, chkLogin: false, url: '/MsSys/BhzSys/GetBhzSmsTwoData', params: $scope.paramsData }, function (data) {

                //console.log(JSON.stringify(data));

                $scope.model_two = data;

            });
        };
        
        //加载3
        $scope.queryData3 = function () {

            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: false, chkLogin: false, url: '/MsSys/BhzSys/GetBhzSmsThreeData', params: $scope.paramsData }, function (data) {

                //console.log(JSON.stringify(data));

                $scope.model_three = data;

            });
        };
        

        //保存1
        $scope.saveSmsData1 = function () {

            if (!QBO.frm.chkForm("thisForm1")) return;

            var model_one = angular.copy($scope.model_one);

            QBO.plu.ng.http({
                http: $http,
                method: 'POST',
                isLoad: true,
                chkLogin: true,
                url: 'SaveBhzSmsOneData',
                params: $scope.paramsData,
                data: model_one
            }, function (result) {

                //console.log(JSON.stringify(result));

                QBO.dia.msg(result.Msg, { icon: result.Result === 100 ? 'right' : 'error' });

            });

        }
        
        //保存2
        $scope.saveSmsData2 = function () {

            if (!QBO.frm.chkForm("thisForm2")) return;

            var model_two = angular.copy($scope.model_two);

            if (model_two.SetTwoList.length == 0) {
                QBO.dia.msg('请新增发送对象！', 'info');
                return;
            }

            QBO.plu.ng.http({
                http: $http,
                method: 'POST',
                isLoad: true,
                chkLogin: true,
                url: 'SaveBhzSmsTwoData',
                params: $scope.paramsData,
                data: model_two
            }, function (result) {

                QBO.dia.msg(result.Msg, { icon: result.Result === 100 ? 'right' : 'error' });

            });

        }

        //保存3
        $scope.saveSmsData3 = function () {

            if (!QBO.frm.chkForm("thisForm3")) return;

            var model_three = angular.copy($scope.model_three);

            if(model_three.Hours<0 || model_three.Hours>=23)
            {
                QBO.dia.msg('发送时间点请填写0-23的整数！', 'info');
                return;
            }
            
            QBO.plu.ng.http({
                http: $http,
                method: 'POST',
                isLoad: true,
                chkLogin: true,
                url: 'SaveBhzSmsThreeData',
                params: $scope.paramsData,
                data: model_three
            }, function (result) {

                QBO.dia.msg(result.Msg, { icon: result.Result === 100 ? 'right' : 'error' });

            });

        }
                

        //是否生效1       
        $scope.selectIsEnable1 = function (value, $event) {

            var thisObj = $($event.target);

            if (thisObj.is(':checked')) {
                $scope.model_one.IsEnable = true;
            }
            else {
                $scope.model_one.IsEnable = false;
            }

        }

        //是否生效2      
        $scope.selectIsEnable2 = function (index, $event) {

            var thisObj = $($event.target);

            if (thisObj.is(':checked')) {
                $scope.model_two.SetTwoList[index].IsEnable = true;
            }
            else {
                $scope.model_two.SetTwoList[index].IsEnable = false;
            }

        }

        //是否生效3    
        $scope.selectIsEnable3 = function (value, $event) {

            var thisObj = $($event.target);

            if (thisObj.is(':checked')) {
                $scope.model_three.IsEnable = true;
            }
            else {
                $scope.model_three.IsEnable = false;
            }

        }


        //获取已设置参数的试验室列表1
        $scope.loadItems1 = function () {
            if ($scope.items1 != null) return;
            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: false, url: '/MsSys/BhzSys/GetSmsSetOneLabs' }, function (data) {
                $scope.items1 = data;
            });
        };

        //获取已设置参数的试验室列表2
        $scope.loadItems2 = function () {
            if ($scope.items2 != null) return;
            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: false, url: '/MsSys/BhzSys/GetSmsSetTwoLabs' }, function (data) {
                $scope.items2 = data;
            });
        };

        //获取已设置参数的试验室列表3
        $scope.loadItems3 = function () {
            if ($scope.items3 != null) return;
            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: false, url: '/MsSys/BhzSys/GetSmsSetThreeLabs' }, function (data) {
                $scope.items3 = data;
            });
        };


        //选择复制1
        $scope.selItem1 = function (item) {

            var paramsItem = {
                "LoadApi": 1,
                "TargetId": item.OrganID
            }

            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: true, url: '/MsSys/BhzSys/GetBhzSmsOneData', params: paramsItem }, function (data) {
                $scope.model_one = data;
                $scope.model_one.OrganID = $scope.paramsData.TargetId;
                //console.log(JSON.stringify($scope.model_one))
            });

        }

        //选择复制2
        $scope.selItem2 = function (item) {

            var paramsItem = {
                "LoadApi": 1,
                "TargetId": item.OrganID
            }

            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: true, url: '/MsSys/BhzSys/GetBhzSmsTwoData', params: paramsItem }, function (data) {

                var TargetId= $scope.paramsData.TargetId;

                $scope.model_two = data;
                $scope.model_two.TargetId = TargetId;

                $scope.model_two.NewModel.OrganID = TargetId;

                var list = $scope.model_two.SetTwoList;

                if (list != null) {
                    for (var key in list) {
                        list[key].OrganID = TargetId;
                    }
                }

                //console.log(JSON.stringify($scope.model_two))
            });

        }

        //选择复制3
        $scope.selItem3 = function (item) {

            var paramsItem = {
                "LoadApi": 1,
                "TargetId": item.OrganID
            }

            QBO.plu.ng.http({ http: $http, method: 'GET', isLoad: true, chkLogin: true, url: '/MsSys/BhzSys/GetBhzSmsThreeData', params: paramsItem }, function (data) {
                $scope.model_three = data;
                $scope.model_three.OrganID = $scope.paramsData.TargetId;
                //console.log(JSON.stringify($scope.model_three))
            });

        }


        //删除
        $scope.deleteModelTwo = function (item) {
            $scope.model_two.SetTwoList.splice($scope.model_two.SetTwoList.indexOf(item), 1);
        }

        //添加
        $scope.addModelTwo = function () {

            var item = angular.copy($scope.model_two.NewModel);

            $scope.model_two.SetTwoList.push(item);
        }


    }

    //初始化ng查询数据
    function initNgQuery($scope, $http, $timeout) {

        if ($scope.paramsData.LoadApi == 1) {
            $scope.queryData();       
        }

       
    }

 
</script>



