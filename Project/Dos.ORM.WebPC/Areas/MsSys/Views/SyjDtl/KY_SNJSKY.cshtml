                <td>
                    <input type="text" id="txlq2" style="width: 100%"  value="@Model.LQ" readonly="readonly" />
                </td>
            </tr>
            <tr>
                <td style="height: 25px;">试验尺寸</td>
                <td>
                    <input type="text" id="tsycc2" style="width: 100%"  value="@Model.SJCC"  readonly="readonly" />
                </td>
                <td  ></td>
                <td></td>
            </tr>
            <tr>
                <td style="height: 25px;">工程部位及用途</td>
                <td colspan="3">
                    <input type="text" id="tgcbw2" style="width: 100%" value="@Model.CJMC"  readonly="readonly" />
                </td>
            </tr>
            <tr>
                <td colspan="4" style="padding: 0px; height: 130px;">
                    <table id="Table6" style="WIDTH: 100%; border: none; HEIGHT: 100%; TEXT-ALIGN: center; BORDER-BOTTOM-STYLE: none"
                           bordercolor="black" cellspacing="0" cellpadding="1" align="center" border="1" frame="void">
                        <tr>
                            <td colspan="7" class="lastco">抗压强度</td>
                        </tr>
                        <tr>
                            <td style="width: 25%;">试件编号</td>
                            <td>1</td>
                            <td>2</td>
                            <td>3</td>
                            <td>4</td>
                            <td>5</td>
                            <td class="lastco">6</td>
                        </tr>
                        <tr>
                            <td>破坏荷载(kN)</td>
                            @for (int i = 0; i < 5; i++)
                            {

                                <td>
                                    <input type="text" id="tkylz@(i+1)" style="width: 100%" value="@(Model.Yljs.Count > i && Model.Yljs[i] != null ? Model.Yljs[i].KYLZ : "")" readonly="readonly" />
                                </td>
                            }
                            
                            <td class="lastco">
                                <input type="text" id="tkylz6" style="width: 100%" value="@(Model.Yljs.Count >5 && Model.Yljs[5] != null ? Model.Yljs[5].KYLZ : "")" readonly="readonly" />
                            </td>
                        </tr>
                        <tr>
                            <td>抗压强度测值(MPa)</td>
                            @for (int i = 0; i < 5; i++)
                            {

                                <td>
                                    <input type="text" id="tkyqd@(i+1)" style="width: 100%" value="@(Model.Yljs.Count > i && Model.Yljs[i] != null ? Model.Yljs[i].KYQD: "")" readonly="readonly" />
                                </td>
                            }
                            
                            <td class="lastco">
                                <input type="text" id="tkyqd6" style="width: 100%" value="@(Model.Yljs.Count > 5 && Model.Yljs[5] != null ? Model.Yljs[5].KYQD: "")" readonly="readonly" />
                            </td>
                        </tr>
                        <tr class="lastro">
                            <td>抗压强度测定值(MPa)</td>
                            <td colspan="6" class="lastco">
                                <input type="text" id="tkyyxqd" style="width: 100%" value="@Model.QDDBZ"  readonly="readonly" />
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td colspan="4" style="padding: 0px;">
                    <table id="Table3" style="WIDTH: 100%; border: none; HEIGHT: 100%; TEXT-ALIGN: center; BORDER-BOTTOM-STYLE: none"
                           bordercolor="black" cellspacing="0" cellpadding="1" align="center" border="1" frame="void"> 
                        @for (int i = 0; i < 3; i++)
                        {
                            <tr>
                                <td style="width: 5%;">
                                    试<br />
                                    件<br />
                                    @(i * 2 + 1)<br />
                                    的<br />
                                    数<br />
                                    据<br />
                                    图<br />
                                    形
                                </td>
                                <td style="width: 45%;">
                                    <a id="kylink(@i*2)+1" title="查看原图" target="_blank">
                                        <img id="kypic@(i*2+1)" src="@(pic[i*2+1]) " alt="" style="width: 90%; max-height: 210px;" />
                                    </a>
                                    <input id="txtkyLabel@(i*2+1)" style="display: none;" value="@labels[1+i*2]" />
                                    <input id="txtkyData@(i*2+1)" style="display: none;" value="@txtDatas[1+i*2]" />
                                    <canvas id="kycanvas@(i*2+1)" style="width: 95%; display: none; margin: 0 auto;"></canvas>
                                </td>
                                <td style="width: 5%;">
                                    试<br />
                                    件<br />
                                    @(i * 2 + 2)<br />
                                    的<br />
                                    数<br />
                                    据<br />
                                    图<br />
                                    形
                                </td>
                                <td class="lastco">
                                    <a id="kylink @(i*2+2)" title="查看原图" target="_blank">
                                        <img id="kypic @(i*2+2)" src="@pic[1+i*2] " alt="" style="width: 90%; max-height: 210px;" />
                                    </a>
                                    <input id="txtkyLabel@(i*2+2)" style="display: none;" value="@labels[1+i*2]" />
                                    <input id="txtkyData@(i*2+2)" style="display: none;" value="@txtDatas[1+i*2]" />
                                    <canvas id="kycanvas@(i*2+2)" style="width: 95%; display: none; margin: 0 auto;"></canvas>
                                </td>
                            </tr>
                        } 
                    </table>
                </td>
            </tr>
        </table>


    </form>
</body>
</html>


